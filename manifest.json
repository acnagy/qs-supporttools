{
  "name": "Animated Page Action",
  "description": "This extension adds an animated browser action to the toolbar.",
  "version": "1.2",
  "background": {
    "scripts": ["background.js"]
  },
  "page_action": {
    "default_title": "QuickSchools Trial Finder",
	"default_icon": "icon.png"
  },
  "manifest_version": 2,
  "permissions" : [
	  "tabs"
  ],
  "content_scripts" : [
	  {
		  "matches" : [
			  "http://*.quickschools.com/*"
		  ],
		  "js" : ["contentscript.js"],
	  }
  ]
}
